.img-plus-style {
  height: 120rpx;
  width: 120rpx;
  position: fixed;
  bottom: 200rpx;
  right: 100rpx;
  z-index: 100;
}

image.img-plus-style  {
  border-radius: 100px;
  box-shadow: 0px 0px 5px 1px #ccc;
}
 
.img-style {
  height: 150rpx;
  width: 150rpx;
  position: fixed;
  bottom: 200rpx;
  right: 100rpx;
  opacity: 0;
  z-index: 100;
}

.container{
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  width: 100%;
  height: 100%;
}

.card {
  position: absolute;
  width: 90%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.card image {
  display: block;
  width: 100%;
  border-radius: 6px;
  box-shadow: 0px 0px 10px 3px #ccc;
}

.result_panel {
  position: absolute;
  top: 50rpx;
  left: 50rpx;
}

.result {
  color: white;
  margin-top: 10rpx;
  text-shadow: 0px 0px 5px red;
  font-size: 40rpx;
}

.result text {
  margin-top: 10rpx;
}